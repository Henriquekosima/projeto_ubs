@model AtendenteViewModel

<div class="row">
    <div class="col-12">
        <div class="col-md-6 offset-md-3">
            <form id="form" action="PostAtendente" method="post">
                <div>
                    <h3 class="espaco">Cadastro Atendente</h3>
                </div>
                <div class="form-group">
                    <input type="text" class="form-control sem-borda" required="required" placeholder="Nome"
                           id="nomeAtendente" name="AttendantName">
                </div>
                <div class="form-group">
                    <input type="email" class="form-control sem-borda" required="required" placeholder="E-mail"
                           id="emailAtendente" name="AttendantEmail">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control sem-borda" required="required" placeholder="CPF"
                           id="entradaCPF" name="AttendantCpf">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control sem-borda" required="required" placeholder="Senha"
                           id="senha" name="AttendantPass">
                </div>
                <button type="submit" class="btn btn-primary right sem-borda blue">Cadastrar</button>
            </form>

        </div>

    </div>
</div>